from sippts.sipdump import SipDump
from sippts.lib.params import get_sipdump_args


def main():
    file, ofile = get_sipdump_args()

    s = SipDump()
    s.file = file
    s.ofile = ofile

    s.start()


if __name__ == '__main__':
    main()
